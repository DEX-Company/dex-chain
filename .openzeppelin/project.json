{
  "manifestVersion": "2.2",
  "contracts": {
    "DIDRegistry": "DIDRegistry",
    "DexToken": "DatacraftToken",
    "DirectPurchase": "DirectPurchase",
    "Dispenser": "Dispenser",
    "Provenance": "Provenance",
    "DatacraftToken": "DatacraftToken"
  },
  "dependencies": {
    "@openzeppelin/contracts-ethereum-package": "^2.5.0"
  },
  "name": "datacraft-chain",
  "version": "0.2.0",
  "compiler": {
    "compilerSettings": {
      "optimizer": {
        "enabled": false,
        "runs": "200"
      }
    },
    "typechain": {
      "enabled": false
    },
    "manager": "openzeppelin",
    "artifactsDir": "build/contracts",
    "contractsDir": "contracts",
    "solcVersion": "0.5.16"
  },
  "telemetryOptIn": true
}
